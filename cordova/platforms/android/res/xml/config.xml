<?xml version='1.0' encoding='utf-8'?>
<widget id="com.gkdv.homatisation" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="Device">
        <param name="android-package" value="org.apache.cordova.device.Device" />
    </feature>
    <feature name="startApp">
        <param name="android-package" value="com.lampa.startapp.startApp" />
    </feature>
    <feature name="WebIntent">
        <param name="android-package" value="com.borismus.webintent.WebIntent" />
        <param name="onload" value="true" />
    </feature>
    <feature name="Notification">
        <param name="android-package" value="org.apache.cordova.dialogs.Notification" />
    </feature>
    <feature name="ShortcutPlugin">
        <param name="android-package" value="com.plugins.shortcut.ShortcutPlugin" />
    </feature>
    <feature name="XSpeechRecognizer">
        <param name="android-package" value="com.phonegap.plugins.speech.XSpeechRecognizer" />
    </feature>
    <feature name="TTS">
        <param name="android-package" value="com.wordsbaking.cordova.tts.TTS" />
        <param name="onload" value="true" />
    </feature>
    <name>Homatisation</name>
    <icon src="icon.png" />
    <description>
        App for Homatisation
    </description>
    <author href="https://gpenverne.github.io/homatisation/">
        Gpenverne
    </author>
    <content src="index.html" />
    <access origin="*" />
    <plugin name="com.plugins.shortcut" />
    <plugin name="WebIntent" value="com.borismus.webintent.WebIntent" />
    <plugin name="SpeechRecognition" value="org.apache.cordova.speech.speechrecognition" />
    <plugin name="cordova-plugin-tts" value="cordova-plugin-tts" />
</widget>
